
public class masatierra {
	
	public static void main(String[] args) {
		
		float gravedad, masaTierra, radioTierra;
		
		masaTierra = (float) (5.972*Math.pow(10,24));
		radioTierra = (float) (6371*Math.pow(10, 3));
		
		gravedad = (float) (masaTierra/radioTierra);
		
		System.out.print(gravedad);
	}

}
