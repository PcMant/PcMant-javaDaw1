
public class masatierra {
	
	public static void main(String[] args) {
		
		double gravedad, masaTierra, radioTierra;
		
		masaTierra = (double) (5.972*Math.pow(10,24));
		radioTierra = (double) (6371*Math.pow(10, 3));
		
		gravedad = (double) (masaTierra/Math.pow(radioTierra, 2));
		
		System.out.print(gravedad);
	}

}
